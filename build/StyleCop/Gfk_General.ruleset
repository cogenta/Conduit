<?xml version="1.0" encoding="utf-8"?>
<RuleSet Name="Gfk Rules" Description="These rules focus on the most critical problems in your code, including potential security holes, application crashes, and other important logic and design errors. You should include this rule set in any custom rule set you create for your projects." ToolsVersion="15.0">
  <Rules AnalyzerId="StyleCop.Analyzers" RuleNamespace="StyleCop.Analyzers">
    <Rule Id="SA0001" Action="None" /> <!-- Xml Comment Analysis Disabled (replaces SA1652) https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA0001.md -->
    <Rule Id="SA1005" Action="None" /> <!-- Single line comments should begin with single space https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1005.md -->
    <Rule Id="SA1009" Action="None" /> <!-- Closing parenthesis should be spaced correctly https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1009.md -->
    <Rule Id="SA1011" Action="None" /> <!-- Closing square brackets should be spaced correctly https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1011.md -->
    <Rule Id="SA1028" Action="None" /> <!-- Code should not contain trailing whitespace https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1028.md -->
    <Rule Id="SA1101" Action="None" /> <!-- Prefix local calls with this https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1101.md -->
    <Rule Id="SA1108" Action="None" /> <!-- Block statements should not contain embedded comments https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1108.md -->
    <Rule Id="SA1110" Action="None" /> <!-- Opening parenthesis or bracket should be on declaration line https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1110.md -->
    <Rule Id="SA1111" Action="None" /> <!-- Closing parenthesis should be on line of last parameter https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1111.md -->
    <Rule Id="SA1115" Action="None" /> <!-- Parameter should follow comma https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1115.md -->
    <Rule Id="SA1116" Action="None" /> <!-- Split parameters should start on line after declaration https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1116.md -->
    <Rule Id="SA1117" Action="None" /> <!-- Parameters should be on same line or separate lines https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1117.md -->
    <Rule Id="SA1124" Action="None" /> <!-- Do not use regions https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1124.md -->
    <Rule Id="SA1125" Action="None" /> <!-- Use shorthand for nullable types https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1125.md -->
    <Rule Id="SA1127" Action="None" /> <!-- Generic type constraints should be on their own line https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1127.md -->
    <Rule Id="SA1128" Action="None" /> <!-- Put constructor initializers on their own line https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1128.md -->
    <Rule Id="SA1133" Action="None" /> <!-- Do not combine attributes https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1133.md -->
    <Rule Id="SA1200" Action="None" /> <!-- Using directives should be placed correctly https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1200.md -->
    <Rule Id="SA1201" Action="None" /> <!-- Elements should appear in the correct order https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1201.md -->
    <Rule Id="SA1202" Action="None" /> <!-- Elements should be ordered by access https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1202.md -->
    <Rule Id="SA1203" Action="None" /> <!-- Constants should appear before fields https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1203.md -->
    <Rule Id="SA1204" Action="None" /> <!-- Static elements should appear before instance elements https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1204.md -->
    <Rule Id="SA1208" Action="None" /> <!-- System using directives should be placed before other using directives https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1208.md -->
    <Rule Id="SA1214" Action="None" /> <!-- Readonly fields should appear before non-readonly fields https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1214.md -->
    <Rule Id="SA1309" Action="None" /> <!-- Field names should not begin with underscore https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1309.md -->
    <Rule Id="SA1310" Action="None" /> <!-- Field names should not contain underscore https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1310.md -->
    <Rule Id="SA1311" Action="None" /> <!-- Static readonly fields should begin with upper-case letter https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1311.md -->
    <Rule Id="SA1413" Action="None" /> <!-- Use trailing comma in multi-line initializers https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1413.md -->
    <Rule Id="SA1501" Action="None" /> <!-- Statement should not be on a single line https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1501.md -->
    <Rule Id="SA1502" Action="None" /> <!-- Element should not be on a single line https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1502.md -->
    <Rule Id="SA1512" Action="None" /> <!-- Single-line comments should not be followed by blank line https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1512.md -->
    <Rule Id="SA1515" Action="None" /> <!-- Single-line comment should be preceded by blank line https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1515.md -->
    <Rule Id="SA1516" Action="Info" /> <!-- Elements should be separated by blank line https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1516.md -->
    <Rule Id="SA1600" Action="None" /> <!-- Elements should be documented - (covered by CS1591) https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1600.md -->
    <Rule Id="SA1610" Action="None" /> <!-- Property documentation should have value text https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1610.md -->
    <Rule Id="SA1623" Action="None" /> <!-- Property summary documentation should match accessors https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1623.md -->
    <Rule Id="SA1629" Action="None" /> <!-- Documentation text should end with a period https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1629.md -->
    <Rule Id="SA1633" Action="None" /> <!-- File should have header https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1633.md -->
    <Rule Id="SA1642" Action="None" /> <!-- Constructor summary documentation should begin with standard text https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1642.md -->
    <Rule Id="SA1643" Action="None" /> <!-- Destructor summary documentation should begin with standard text https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1643.md -->
    <Rule Id="SA1652" Action="None" /> <!-- Enable Xml Documentation Output https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1652.md -->
    <Rule Id="SX1309" Action="Warning" /> <!-- Field names should begin with underscore https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SX1309.md -->
    <Rule Id="SX1309S" Action="Warning" /> <!-- Static field names should begin with underscore https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SX1309S.md -->
  </Rules>
  <Rules AnalyzerId="Microsoft.CodeQuality.Analyzers" RuleNamespace="Microsoft.CodeQuality.Analyzers">
    <Rule Id="CA1032" Action="Info" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca1032-implement-standard-exception-constructors?view=vs-2017 -->
    <Rule Id="CA1034" Action="Info" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca1034-nested-types-should-not-be-visible?view=vs-2017 -->
    <Rule Id="CA1054" Action="Info" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca1054-uri-parameters-should-not-be-strings?view=vs-2017 -->
    <Rule Id="CA1056" Action="Info" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca1056-uri-properties-should-not-be-strings?view=vs-2017 -->
    <Rule Id="CA1063" Action="Info" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca1063-implement-idisposable-correctly?view=vs-2017 -->
    <Rule Id="CA1707" Action="None" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca1707-identifiers-should-not-contain-underscores?view=vs-2017 -->
    <Rule Id="CA1710" Action="None" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca1710-identifiers-should-have-correct-suffix?view=vs-2017 -->
    <Rule Id="CA1716" Action="Info" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca1716-identifiers-should-not-match-keywords?view=vs-2017 -->
    <Rule Id="CA1822" Action="Info" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca1822-mark-members-as-static?view=vs-2017 -->
    <Rule Id="CA2007" Action="None" /> <!-- Do not directly await a Task without calling ConfigureAwait -->
    <Rule Id="CA2227" Action="Info" /> <!-- https://docs.microsoft.com/en-us/visualstudio/code-quality/ca2227-collection-properties-should-be-read-only?view=vs-2017 -->
  </Rules>
  <Rules AnalyzerId="Microsoft.VisualStudio.Threading.Analyzers" RuleNamespace="Microsoft.VisualStudio.Threading.Analyzers">
    <Rule Id="VSTHRD002" Action="Info" /> <!-- Avoid problematic synchronous waits https://github.com/Microsoft/vs-threading/blob/master/doc/analyzers/VSTHRD002.md -->
  </Rules>
</RuleSet>